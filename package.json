    "class-validator": "0.12.2",
    "colors": "1.4.0",
    "compression": "1.7.4",
    "connect-redis": "4.0.4",
    "core-js": "3.17.3",
    "dotenv": "10.0.0",
    "errorhandler": "1.5.1",
    "event-source-polyfill": "1.0.12",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "ioredis": "4.17.3",
    "lodash": "4.17.15",
    "method-override": "3.0.0",
    "moment": "2.29.1",
