{
  "name": "vascomtest",
  "version": "1.0.0",
  "repository": "https://github.com/MuhamadIsmuaji/vascomtest.git",
  "author": "Muhamad Ismuaji Prajitno <muhamadismuaji@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "14.15.0"
  },
  "scripts": {
    "start": "yarn migrations:run && node --expose-gc dist/src/index",
    "build": "yarn build:back && yarn build:front",
    "build:front": "NODE_ENV=production webpack",
    "build:back": "rimraf ./dist && tsc -p ./app/tsconfig.json && yarn postbuild:back && yarn migrations:validate",
    "dev": "yarn dev:back & yarn dev:front",
    "dev:front": "NODE_ENV=development webpack --watch & node config/bs.config.js",
    "dev:back": "nest start --watch -c ./app/nest-cli.json -p ./app/tsconfig.json",
    "lint": "eslint",
    "lint:front": "eslint --fix --ext .ts,.js,.vue ./client",
    "lint:back": "eslint --fix --ext .ts ./app",
    "pretty": "prettier --write ./app ./client/src",
    "migrations:run": "node dist/scripts/run-migrations.script.js",
    "migrations:up": "sequelize-cli db:migrate",
    "migrations:down": "sequelize-cli db:migrate:undo",
    "migrations:generate": "node dist/scripts/gen-migration.script.js",
    "migrations:validate": "node dist/scripts/validate-migrations.script.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.1.1",
    "@nestjs/cli": "8.1.1",
    "@nestjs/common": "8.0.6",
